<div class="header">
	<div class="container">
		<!--><img></img>-->
		<h1>Messenger</h1>
	</div>
</div>
	
<div class="messages">
	<div class="container">
		<% @messages.each do |message| %>
		<%= link_to 'New Message', "messages/new" %>
		<p class="content"><%= message.content %></p>
		<p class="time"<%= message.created_at%></p>
	</div>
	<% end %>
</div>
